<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Userpermission" table="userpermission">
    <indexes>
      <index name="User_ID" columns="User_ID"/>
      <index name="Permission_ID" columns="Permission_ID"/>
      <index name="FK1_UserPermission_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_UserPermission_ModifiedBy" columns="ModifiedBy"/>
    </indexes>
    <id name="userpermissionId" type="integer" column="UserPermission_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="permission" target-entity="Permission" fetch="LAZY">
      <join-columns>
        <join-column name="Permission_ID" referenced-column-name="Permission_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="User_ID" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

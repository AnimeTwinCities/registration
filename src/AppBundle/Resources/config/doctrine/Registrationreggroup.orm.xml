<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Registrationreggroup" table="RegistrationRegGroup">
    <indexes>
      <index name="FK1_RegistrationRegGroup_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_RegistrationRegGroup_ModifiedBy" columns="ModifiedBy"/>
      <index name="FK2_RegistrationRegGroup_Registration_ID" columns="Registration_ID"/>
      <index name="FK2_RegistrationRegGroup_RegGroup_ID" columns="RegGroup_ID"/>
    </indexes>
    <id name="registrationreggroupId" type="integer" column="RegistrationRegGroup_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="reggroup" target-entity="Reggroup" fetch="LAZY">
      <join-columns>
        <join-column name="RegGroup_ID" referenced-column-name="RegGroup_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="registration" target-entity="Registration" fetch="LAZY">
      <join-columns>
        <join-column name="Registration_ID" referenced-column-name="Registration_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Registration" table="registration">
    <indexes>
      <index name="FK1_Registration_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_Registration_ModifiedBy" columns="ModifiedBy"/>
      <index name="FK2_Registration_RegistrationType_ID" columns="RegistrationType_ID"/>
      <index name="FK2_Registration_RegistrationStatus_ID" columns="RegistrationStatus_ID"/>
      <index name="FK2_Registration_Event_ID" columns="Event_ID"/>
      <index name="FK3_TransferedTo" columns="TransferedTo"/>
    </indexes>
    <id name="registrationId" type="integer" column="Registration_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="number" type="string" column="Number" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="confirmationnumber" type="string" column="ConfirmationNumber" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="FirstName" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="LastName" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="middlename" type="string" column="MiddleName" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="Address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="Address2" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="City" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="State" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="Zip" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="Phone" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthday" type="datetime" column="Birthday" nullable="true"/>
    <field name="badgename" type="string" column="BadgeName" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contactVolunteer" type="string" column="contact_volunteer" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
    <field name="contactNewsletter" type="string" column="contact_newsletter" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
    <field name="xml" type="string" column="XML" length="8296" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="event" target-entity="Event" fetch="LAZY">
      <join-columns>
        <join-column name="Event_ID" referenced-column-name="Event_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="registrationstatus" target-entity="Registrationstatus" fetch="LAZY">
      <join-columns>
        <join-column name="RegistrationStatus_ID" referenced-column-name="RegistrationStatus_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="registrationtype" target-entity="Registrationtype" fetch="LAZY">
      <join-columns>
        <join-column name="RegistrationType_ID" referenced-column-name="RegistrationType_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="transferedto" target-entity="Registration" fetch="LAZY">
      <join-columns>
        <join-column name="TransferedTo" referenced-column-name="Registration_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <indexes>
      <index name="FK1_User_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_User_ModifiedBy" columns="ModifiedBy"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="Login" columns="Login"/>
      <unique-constraint name="Nickname" columns="Nickname"/>
    </unique-constraints>
    <id name="userId" type="integer" column="User_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="openidIdentity" type="string" column="openid_identity" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="Login" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nickname" type="string" column="Nickname" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="Password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="FirstName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="LastName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="string" column="Position" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="Description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disabled" type="string" column="Disabled" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
    <field name="photopath" type="string" column="PhotoPath" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Registrationshirt" table="RegistrationShirt">
    <indexes>
      <index name="FK1_RegistrationShirt_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_RegistrationShirt_ModifiedBy" columns="ModifiedBy"/>
      <index name="FK2_RegistrationShirt_Registration_ID" columns="Registration_ID"/>
      <index name="FK2_RegistrationShirt_Shirt_ID" columns="Shirt_ID"/>
    </indexes>
    <id name="registrationshirtId" type="integer" column="RegistrationShirt_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="registration" target-entity="Registration" fetch="LAZY">
      <join-columns>
        <join-column name="Registration_ID" referenced-column-name="Registration_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="shirt" target-entity="Shirt" fetch="LAZY">
      <join-columns>
        <join-column name="Shirt_ID" referenced-column-name="Shirt_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

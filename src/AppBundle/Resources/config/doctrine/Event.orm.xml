<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Event" table="event">
    <indexes>
      <index name="Year" columns="Year"/>
      <index name="Active" columns="Active,Public"/>
      <index name="FK1_Event_CreatedBy" columns="CreatedBy"/>
      <index name="FK2_Event_ModifiedBy" columns="ModifiedBy"/>
    </indexes>
    <id name="eventId" type="integer" column="Event_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="year" type="string" column="Year" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startdate" type="datetime" column="StartDate" nullable="true"/>
    <field name="enddate" type="datetime" column="EndDate" nullable="true"/>
    <field name="theme" type="string" column="Theme" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attendancecap" type="integer" column="AttendanceCap" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="active" type="string" column="Active" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
    <field name="public" type="string" column="Public" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">false</option>
      </options>
    </field>
    <field name="finalattendance" type="integer" column="FinalAttendance" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createddate" type="datetime" column="CreatedDate" nullable="true"/>
    <field name="modifieddate" type="datetime" column="ModifiedDate" nullable="true"/>
    <many-to-one field="createdby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="CreatedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="modifiedby" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="ModifiedBy" referenced-column-name="User_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
